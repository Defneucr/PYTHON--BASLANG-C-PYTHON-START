pass_word = input("8 ile 10 haneli en az iki harf ve bir karakter içeren şifre belirleyiniz")
harf_sayisi = 0
karakter_sayisi = 0

if(len(pass_word)<8)|(len(pass_word)>10):
    print("şifreniz 8 ile 10 haneli olmadığı için kabul edilmemiştir")

for i in pass_word:
    if i.isalpha():
        harf_sayisi = harf_sayisi + 1
for i in pass_word:
    if i in("+","/","-","*",".","_","-","?","#","%","&","="):
        karakter_sayisi = karakter_sayisi + 1
if harf_sayisi < 2 :
    print("şifreniz en az 2 harf içermediği için kabul edilmemiştir")
elif karakter_sayisi < 1:
    print("şifreniz en az 1 karakter içermediği için kabul edilmemiştir")
else:
    print("şifreniz kabul edilmiştir...")